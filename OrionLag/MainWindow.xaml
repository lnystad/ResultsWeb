        <Menu DockPanel.Dock="Top">
            <MenuItem Header="Options">
                <MenuItem Header="_Open"/>
            </MenuItem>
            <MenuItem Header="GenererLag">
                <MenuItem Header="_Open" Click="InputMenuItemOpen_Click"/>
            </MenuItem>
            <MenuItem Header="KjørLag">
                <MenuItem Header="_FinFelt" Click="InputMenuItemFinFeltOpen_Click"/>
                <MenuItem Header="_GrovFeltFelt" Click="InputMenuItemGrovFeltOpen_Click"/>
            </MenuItem>
        </Menu>
        <StackPanel>

        </StackPanel>
    </DockPanel>
</Window>
